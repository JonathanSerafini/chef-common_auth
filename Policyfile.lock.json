  "default_attributes": {
    "common_auth": {
      "groups": {
        "config": {
          "devops": true
        }
      },
      "sudoers": {
        "ubuntu": {
          "user": "ubuntu",
          "runas": "root",
          "nopasswd": true
        },
        "deploy": {
          "group": "deploy",
          "runas": "root",
          "nopasswd": true,
          "commands": [
            "/usr/bin/chef-client"
          ]
        },
        "90-cloud-init-users": {
          "action": "remove"
        }
      }
    },
    "authorization": {
      "sudo": {
        "groups": [
          "devops"
        ]
      }
    },
    "openssh": {
      "server": {
        "allow_groups": [
          "devops",
          "deploy"
        ]
      }
    }
  },
